
<%= simple_form_for [:admin, @medicine_manufacturer] do |f| %>

  <div class="group">
    <%= f.input :name %>
  </div>

  <div class="group">
    <%= f.input :website %>
  </div>

  <div class="group">
    <%= f.input :contact_info %>
  </div>


  <%= f.submit "Submit", data: { disable_with: "Submitting..." } %>

<% end %>
